page{
	background-color: #eee;
}
.van-cell-group{
	margin: 20rpx 0;
}
.van-cell .requiredSymbol{
	color: red;
}
.baseInfo .inputRight{
	width: 100%;
	text-align: left;
	color: #333;
}
.baseInfo .floor{
	display: flex;
	color: #333;
}
.baseInfo .floorInput{
	border: 1px solid #ccc;
	width: 60rpx;
	border-radius: 10rpx;
}

.detailInfo .cellValue{
	display: flex;
	flex-direction: row;
	align-items: center;
	height: 60rpx;
}
.detailInfo .content{
	border: 5rpx solid #eee;
	border-radius: 10rpx;
	padding: 10rpx;
	font-size: 28rpx;
}
.van-switch{
	margin-top: 10rpx;
}
.van-dropdown-menu{
	height: 30rpx;
}

.submitBtn{
	width: 80%;
	background-color: #3a9ced;
	color: #fff;
	margin-bottom: 30rpx;
}

.saveBtn{
	width: 80%;
	background-color: #e0e0e0;
	color: #333;
	margin-bottom: 10rpx;
}
.delSaveBtn{
	width: 80%;
	background-color: #e0e0e0;
	color: #333;
	margin-bottom: 30rpx;
}